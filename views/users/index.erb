<div class="CSSTableGenerator" >
    <table >
        <thead>
            <td>Company Name</td>
            <td>Address</td>
            <td>Phone</td>
            <td>Email</td>
            <td>Business Description</td>
        </thead>

<% @users.each do |user| %>
        <tr>
            <td><a href="/users/<%= user.id %>"><%=h user.name %></a></td>
            <td><%=h user.address %></td>
            <td><%=h user.phone %></td>
            <td><%=h user.email %></td>
            <td><%=h user.description %></td>
         </tr>
        </tr>
        <% end %>
    </table>
</div>
